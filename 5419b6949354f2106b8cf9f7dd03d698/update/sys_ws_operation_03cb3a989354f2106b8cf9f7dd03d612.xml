        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {
    try {
        var body = request.body.data;

        var log = new GlideRecord('x_1522524_stayin_0_keepalive_log');
        log.initialize();
        log.u_run_time = new GlideDateTime();
        log.u_status_code = body.status || 200;
        log.u_message = body.message || 'Keep-alive ping received';
        log.u_source = body.source || 'unknown';
        var sysId = log.insert();

        response.setStatus(201);
        return { result: "Keep-alive recorded", sys_id: sysId };
    } catch (e) {
        response.setStatus(500);
        return { error: e.message };
    }
})(request, response);
]]></operation_script>
